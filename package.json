{
  "name": "eda-browser-extension",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && node build.mjs",
    "watch": "node build.mjs --watch",
    "lint": "web-ext lint --source-dir dist",
    "typecheck": "tsc --noEmit",
    "package": "npm run build && npm run lint && web-ext build --source-dir dist --overwrite-dest",
    "package:firefox": "npm run build && npm run lint && web-ext sign --source-dir dist --channel unlisted",
    "dev": "npm run build && web-ext run --source-dir dist",
    "clean": "rm -rf dist web-ext-artifacts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.9",
    "@mui/material": "^6.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.7.0",
    "web-ext": "^8.3.0"
  }
}
